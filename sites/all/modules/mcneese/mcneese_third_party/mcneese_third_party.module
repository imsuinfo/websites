/**
 * Use a local copy of facebooks javascript API.
 *
 * This was required by the "bookstore" frontpage.
 *
 * The facebook js is from:
 * - https://connect.facebook.net/en_US/sdk.js
 * - https://connect.facebook.net/en_US/all.js (old)
 * - https://github.com/facebookarchive/connect-js
 */
function mcneese_third_party_add_facebook_connect_js() {
  $module_path = drupal_get_path('module', 'mcneese_third_party');

  //drupal_add_js($module_path . '/js/facebook-connect-all.js', array('type' => 'file', 'group' => JS_DEFAULT, 'preprocess' => TRUE));
  drupal_add_js($module_path . '/js/facebook-connect-sdk.js', array('type' => 'file', 'group' => JS_DEFAULT, 'preprocess' => TRUE));
}

/**
 * Use a local copy of facebooks javascript API (legacy)
 *
 * This was required by the "bookstore" frontpage.
 * This file is questionable given that its SSL certificate reports failures.
 *
 * The facebook js is from:
 * - http://static.ak.connect.facebook.com/connect.php/en_US
 */
function mcneese_third_party_add_facebook_connect_legacy_js() {
  $module_path = drupal_get_path('module', 'mcneese_third_party');

  drupal_add_js($module_path . '/js/facebook-connect-legacy.js', array('type' => 'file', 'group' => JS_DEFAULT, 'preprocess' => TRUE));
}

/**
 * Initialize the appropriate facbook javascript.
 *
 * This must be called after mcneese_third_party_add_facebook_connect_js().
 *
 * @param string $id
 *   The unique facebook init string.
 *
 * @see: mcneese_third_party_add_facebook_connect_js()
 * @see: https://developers.facebook.com/docs/javascript/quickstart/v2.5
 * @see: https://developers.facebook.com/docs/javascript/examples
 */
function mcneese_third_party_do_facebook_connect_js($id) {
  if (!is_string($id)) {
    if (class_exists('cf_error')) {
      cf_error::invalid_string('id');
    }
    return;
  }

  //$init_js = 'FB.init("' . $id . '");';
  $init_js = 'window.fbAsyncInit = function() {';
  $init_js .= '  FB.init({';
  $init_js .= '    appId : \'' . $id . '\',';
  $init_js .= '    xfbml : true,';
  $init_js .= '    version : \'v2.4\'';
  $init_js .= '  });';
  $init_js .= '};';

  drupal_add_js($init_js, array('type' => 'inline', 'preprocess' => FALSE));
}


/**
 * Automatically load the acalog settings.
 *
 * @todo: As this gets larger, it may need its own folder and module.
 *
 * Information is provided here as ajax:
 * - http://catalog.mcneese.edu/widget-api/catalogs/
 *
 * @param string $host_name
 *   Name of the acalog server.
 * @param string $what
 *   (optional) What specific data to get.
 *   When NULL, all data is loaded.
 *   @todo: implement this.
 * @param array $settings
 *   (optional) additional arguments to pass based on the $what parameter.
 *   @todo: implement this.
 *
 * @return string|bool
 *   Processed HTML on success, FALSE otherwise.
 */
function mcneese_third_party_acalog_catalog_get($host_name = 'catalog.mcneese.edu', $what = NULL, $settings = array()) {
  // @todo: process $what and $settings and pass as necessary to the url used for drupal_http_request().

  // load the catalog list. @todo: cache all of this, with a 24 hour timeout.
  $result = drupal_http_request('http://' . $host_name . '/widget-api/catalogs/');

  if (!isset($result->code) && $result->code != 200) {
    // @todo: ideally, load the cached result on http errors (therefore keep a more long term cache backup for http failures and replace it only on success).
    // for now just return FALSE.
    return FALSE;
  }

  $json_data = json_decode($result->data);
  if (!is_object($json_data)) {
    // @todo: ideally, load the cached result on http errors (therefore keep a more long term cache backup for http failures and replace it only on success).
    // for now just return FALSE.
    return FALSE;
  }

  // @todo: until $what and $settings are implemented, this will just assume all 'catalogs' content.
  $current_catalog = NULL;
  $markup = '<h3>Available Catalogs</h3>';
  $markup .= '<ul>';
  foreach ($json_data->{'catalog-list'} as $entry) {
    $markup .= '<li>' . check_plain($entry->name) . '</li>';

    // try to get the most recently active catalog and use that.
    if (!isset($entry->archive) || $entry->archive != 1) {
      if (isset($entry->published) && $entry->published) {
        $current_catalog = $entry;
      }
    }
  }
  unset($entry);
  $markup .= '</ul>';

  if (is_null($current_catalog)) {
    // @todo: handle this case.
    return FALSE;
  }

  // observed catalog structure: (@todo: create cache database using all fields below, note: [catalog-type] as am object implies support for multiple types.)
  /*
  [0] => stdClass Object
    (
        [description] =>
        [catalog-type] => stdClass Object
            (
                [name] => Academic
                [created] => 2015-07-13 16:19:03
